{
  "name": "fetch_and_summarize",
  "description": "Fetch content from a URL and have an agent summarize or extract specific information",
  "parameters": {
    "type": "object",
    "properties": {
      "url": {
        "type": "string",
        "description": "The URL to fetch content from"
      },
      "extractPrompt": {
        "type": "string",
        "description": "What to extract or summarize from the content"
      }
    },
    "required": ["url", "extractPrompt"]
  },
  "pipeline": [
    {
      "type": "tool",
      "tool": "webfetch",
      "name": "Fetch URL",
      "arguments": "{\"url\": \"{{url}}\"}",
      "handoff": "none"
    },
    {
      "type": "agent",
      "agent": "general",
      "name": "Extract Info",
      "prompt": "From this web content:\n\n{{_lastOutput}}\n\n{{extractPrompt}}",
      "handoff": "none"
    }
  ]
}
